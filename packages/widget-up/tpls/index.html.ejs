<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Document</title>
  <link rel="stylesheet" href="./index.css" />
  <% externalStylesheets.forEach(function(stylesheet) { %>
    <link rel="stylesheet" href="<%= stylesheet %>" />
  <% }); %>
  <% if (includeCSS) { %>
    <link rel="stylesheet" href="./umd/bundle.css" />
  <% } %>
  <% if (inlineStyles) { %>
    <style>
      <%= inlineStyles %>
    </style>
  <% } %>
</head>
<body>
  <div class="widget-up-demo-container">
    <% if (menus) { %>
      <div class="widget-up-demo-menu-container">
        <ul>
          <% function renderMenu(menus, level = 0) { %>
            <% menus.forEach(function(menu) { %>
              <li class="level-<%= level %>">
                <%= menu.name %>
                <% if (menu.children && menu.children.length > 0) { %>
                  <ul>
                    <%= renderMenu(menu.children, level + 1) %>
                  </ul>
                <% } %>
              </li>
            <% }); %>
          <% } %>
          <%= renderMenu(menus) %>
        </ul>
      </div>
    <% } %>
    <div class="widget-up-demo-root-container">
      <div id="root"></div>
    </div>
    <div class="widget-up-demo-right-space"></div>
  </div>
  <% scriptTags.forEach(function(tag) { %>
    <script src="<%= tag.src %>"></script>
    <script>
      <%= tag.global %>
    </script>
  <% }); %>
  <script src="<%= bundleSrc %>"></script>
</body>
</html>
