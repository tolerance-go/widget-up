<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <% if (includeCSS) { %>
    <link rel="stylesheet" href="./umd/bundle.css" />
    <% } %>
  </head>
  <body>
    <div id="root"></div>
    <% scriptTags.forEach(function(tag) { %>
    <script src="<%= tag.src %>"></script>
    <script>
      <%= tag.global %>
    </script>
    <% }); %>
    <script src="<%= bundleSrc %>"></script>
  </body>
</html>
