{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [">0.25%", "not dead"],
          "node": "16"
        },
        "modules": false
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["add-import-extension", { "extension": "js" }],
    [
      "alias",
      {
        "@": "."
      }
    ]
  ]
}
