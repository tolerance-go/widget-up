{
  "name": "widget-up-utils",
  "version": "0.6.2",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "bin": {
    "genUMDAsyncEventWrap": "./dist/bin/genUMDAsyncEventWrap/index.js",
    "genUMDAliasWrap": "./dist/bin/genUMDAliasWrap/index.js"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:js\" \"npm run dev:types\"",
    "schema-dev": "schema-builder dev",
    "schema-build": "schema-builder build",
    "build": "npm run clean && npm run build:js && npm run build:types && node dist/rollup-plugins/tsDeclarationAlias/exec.js",
    "build:js": "cross-env NODE_ENV=production babel src --config-file ./babel.config.json --out-dir dist --extensions \".ts\" --ignore \"**/*.test.ts\"",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "dev:js": "cross-env NODE_ENV=production babel src --config-file ./babel.config.json --out-dir dist --extensions \".ts\" --ignore \"**/*.test.ts\" --watch",
    "dev:types": "tsc --project tsconfig.build.json --emitDeclarationOnly --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:update": "node --experimental-vm-modules node_modules/jest/bin/jest.js -u",
    "clean": "rimraf dist",
    "prepublishAll": "npm run test && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "chokidar": "^3.6.0",
    "connect-livereload": "^0.6.1",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "glob": "^10.3.12",
    "livereload": "^0.9.3",
    "open": "^10.1.0",
    "semver": "^7.6.0",
    "strip-json-comments": "^5.0.1",
    "tslib": "^2.6.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/connect-livereload": "^0.6.3",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/livereload": "^0.9.5",
    "@types/node": "^20.12.7",
    "@types/semver": "^7.5.8",
    "@types/yargs": "^17.0.32",
    "axios-mock-adapter": "^1.22.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-alias": "^1.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.3",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.5"
  },
